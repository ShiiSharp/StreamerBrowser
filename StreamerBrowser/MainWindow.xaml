        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StreamerBrowser"
        mc:Ignorable="d"
        Title="Streamer Browser" Height="96" Width="800"
        Name ="MenuWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="1" Name="MenuBar" IsMainMenu="True" Margin="836,824,-836,-797">
            <Menu Name ="FileMenu">
                ファイル
            </Menu>
            <Menu Name ="EditMenu">
                編集
            </Menu>

        </Menu>
    </Grid>
</Window>
